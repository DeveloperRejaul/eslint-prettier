{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "semi": "error",
    "linebreak-style": "off",
    "no-unused-vars": "off",
    "eol-last": "off",
  }
}
